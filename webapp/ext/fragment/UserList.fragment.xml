<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<Dialog title="List of Users">
		<Table id="isUserList" inset="false" items="{/FLPLogInSet}">
			<columns>
				<Column>
					<Text text="User"/>
				</Column>
								<Column>
					<Text text="Application"/>
				</Column>
				<Column>
					<Text text="Last Login"/>
				</Column>
								<Column>
					<Text text="Browser"/>
				</Column>
								<Column>
					<Text text="Device Type"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem>
					<cells>
						<Text text="{UserID}"/>
						<Text text="{App}"/>
						<Text text="{LastLoginAt}"/>
						<Text text="{Browser}"/>
						<Text text="{DeviceType}"/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Dialog>
</core:FragmentDefinition>