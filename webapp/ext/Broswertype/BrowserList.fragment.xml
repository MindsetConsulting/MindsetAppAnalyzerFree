<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<Dialog title="List of Users ({oBrowserModel>/UniqueUserCount})" resizable="true">
		<Table id="isBrowserList" inset="false" items="{path: 'oBrowserModel>/Users', sorter: {path: 'LastLoginAt',descending: true}}">
			<columns>
				<Column>
					<Text text="Browser"/>
				</Column>
				<Column>
					<Text text="User"/>
				</Column>
				<Column>
					<Text text="Last Login"/>
				</Column>
				<!-- <Column>
					<Text text="Device Type"/>
				</Column> -->
			</columns>
			<items>
				<ColumnListItem>
					<cells>
						<Text text="{oBrowserModel>Browser}"/>
						<Text text="{oBrowserModel>UserID}"/>
						<Text text="{ path: 'oBrowserModel>LastLoginAt', type: 'sap.ui.model.type.DateTime', formatOptions: { source: { pattern: 'yyyyMMddTHHmmss' }, style: 'long', UTC: false }}"/>
						<!-- <Text text="{DeviceType}"/> -->
					</cells>
				</ColumnListItem>
			</items>
		</Table>
		<endButton>
			<Button text="Cancel" press="onCloseDialog" ></Button>
		</endButton>
	</Dialog>
</core:FragmentDefinition>